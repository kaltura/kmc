	<renderers>
		<renderer id="playlistItemRenderer">
			<HBox id="irCont" height="100%" width="100%" x="10" y="10" verticalAlign="top"
				  styleName="Button_upSkin_default">
				<Image id="irImageIrScreen" height="48" width="72" url="{this.thumbnailUrl}"/>
				<VBox height="100%" width="100%" id="labelsHolder" verticalGap="0">
					<HBox id="nameAndDuration" width="100%" height="18">
						<Label id="irLinkIrScreen" height="18" width="100%" text="{this.name}"
							   styleName="itemRendererLabel"/>
						<Label id="irDurationIrScreen" height="18" width="70"
							   text="{formatDate(this.duration, 'NN:SS')}" styleName="itemRendererLabel"/>
					</HBox>
					<Label id="irDescriptionIrScreen" width="240" height="18" text="{this.description}"
						   styleName="itemRendererLabel"/>
					<Label id="irPlaysIrScreen" width="240" height="18" text="Plays: {this.plays}"
						   styleName="itemRendererLabel"/>
					<Label id="irRankIrScreen" width="240" height="18" text="Rank: {this.rank}"
						   styleName="itemRendererLabel"/>
					<Label id="irVotesIrScreen" width="240" height="18" text="{this.votes} votes"
						   styleName="itemRendererLabel"/>
					<Label id="irTagsIrScreen" width="240" height="18" text="Tags: {this.tags}"
						   styleName="itemRendererLabel"/>
					<Label id="irAdminTagsIrScreen" width="240" height="18" text="Categories: {this.categories}"
						   styleName="itemRendererLabel"/>
					<Label id="irCreatedAtIrScreen" width="240" height="18"
						   text="Created at: {formatDate(this.createdAt, 'dd/mm/yyy')}" styleName="itemRendererLabel"/>
					<Label id="irCreatedByIrScreen" width="240" height="18" text="Created by: {this.userScreenName}"
						   styleName="itemRendererLabel"/>
				</VBox>
			</HBox>
		</renderer>
	</renderers>
